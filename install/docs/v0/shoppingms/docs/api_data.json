          {
            "group": "JSON Request Body",
            "type": "Int",
            "optional": true,
            "field": "quantity",
            "description": "<p>Number of items in the shopping List.</p>"
          },
          {
            "group": "JSON Request Body",
            "type": "String",
            "optional": true,
            "field": "measurementUnit",

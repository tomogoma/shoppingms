          },
          {
            "group": "200 JSON Response Body",
            "type": "Int",
            "optional": false,
            "field": "quantity",
            "description": "<p>Number of items to get.</p>"
          },
          {
            "group": "200 JSON Response Body",
            "type": "Boolean",
            "optional": false,
            "field": "inList",
            "description": "<p>True if item is in list, false otherwise.</p>"
